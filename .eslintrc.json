{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "GM": false
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "no-use-before-define": ["error", { "functions": false }],
        "no-param-reassign": ["error", { "props": false }]
    }
}